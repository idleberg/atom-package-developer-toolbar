{
  "name": "package-developer-toolbar",
  "version": "0.9.0",
  "description": "A toolbar aimed at developers of Atom packages and syntax themes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-package-developer-toolbar"
  },
  "keywords": [
    "toolbar",
    "tool-bar",
    "package developer"
  ],
  "main": "./lib/tool-bar",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "open-in-developer-mode",
    "open-package",
    "package-generator",
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "coffeelint-stylish": "^0.1.2",
    "eslint": "^3.7.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.3"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
