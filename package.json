{
  "name": "package-developer-toolbar",
  "version": "0.3.0",
  "description": "A toolbar targeted at developers of Atom packages and syntax themes",
  "license": "MIT",
  "main": "./lib/tool-bar",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-package-developer-toolbar"
  },
  "keywords": [
    "toolbar",
    "tool-bar",
    "package developer"
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.1.1",
    "eslint-config-atom-build": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.2"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
