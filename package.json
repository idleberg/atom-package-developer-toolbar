{
  "name": "package-developer-toolbar",
  "version": "0.11.1",
  "description": "A toolbar aimed at developers of Atom packages and syntax themes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-package-developer-toolbar"
  },
  "keywords": [
    "toolbar",
    "tool-bar",
    "package developer"
  ],
  "main": "./lib/tool-bar",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "browse",
    "local-storage",
    "open-in-developer-mode",
    "open-package",
    "package-generator",
    "satisfy-dependencies",
    "tool-bar"
  ],
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.6.2"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.2.0"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
